_..() {
	local cur opts
	cur="${COMP_WORDS[COMP_CWORD]}"
	opts=$(cd .. ; ls -d */ | $SED 's|/./||')
	COMPREPLY=($(compgen -W "${opts}" -- ${cur}))
}
complete -F _.. ..

_...() {
	local cur opts
	cur="${COMP_WORDS[COMP_CWORD]}"
	opts=$(cd ../.. ; ls -d */ | $SED 's|/./||')
	COMPREPLY=($(compgen -W "${opts}" -- ${cur}))
}
complete -F _... ...
complete -F _... ..2

_git_branches() {
	local cur opts
	cur="${COMP_WORDS[COMP_CWORD]}"
	opts=$(git branch)
	COMPREPLY=($(compgen -W "${opts} auto" -- ${cur}))
}
complete -F _git_branches push
complete -F _git_branches merge
