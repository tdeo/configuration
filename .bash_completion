_..() {
  local cur opts
  cur="${COMP_WORDS[COMP_CWORD]}"
  opts=$(cd .. ; ls -d */ | sed 's|/./||')
  COMPREPLY=($(compgen -W "${opts}" -- ${cur}))
}

_git_branches() {
  local cur opts
  cur="${COMP_WORDS[COMP_CWORD]}"
  opts=$(git branch --format "%(refname)" | sed 's|refs/heads/||')
  COMPREPLY=($(compgen -W "${opts} auto" -- ${cur}))
}

_git_pr() {
  _git_checkout
}
